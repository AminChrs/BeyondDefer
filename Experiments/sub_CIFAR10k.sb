executable = run_CIFAR10k.sh
arguments = $(Process)
error = joblogs_CIFAR10k/$(Process).err.txt
output = joblogs_CIFAR10k/$(Process).out.txt
log = joblogs_CIFAR10k/$(Process).log.txt
getenv = True
request_cpus = 4
request_gpus = 1
request_memory = 100GB
+MaxRunningPrice = 500
+RunningPriceExceededAction = "restart"
queue 1
