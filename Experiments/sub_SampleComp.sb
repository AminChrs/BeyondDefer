executable = run_SampleComp.sh
arguments = $(Process)
error = joblogs_SampleComp/$(Process).err.txt
output = joblogs_SampleComp/$(Process).out.txt
log = joblogs_SampleComp/$(Process).log.txt
getenv = True
request_cpus = 4
request_gpus = 1
request_memory = 100GB
+MaxRunningPrice = 500
+RunningPriceExceededAction = "restart"
queue 1
